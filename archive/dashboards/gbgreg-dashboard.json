{
  "dashboard": {
    "id": null,
    "title": "GBGreg Laboratory Development Environment",
    "tags": ["gbgreg", "laboratory", "c-sharp", "ai", "development"],
    "style": "dark",
    "timezone": "",
    "panels": [
      {
        "id": 1,
        "title": "Container Resource Usage",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "pluginVersion": "12.1.1",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=\"gbgreg\"}[5m]) * 100",
            "legendFormat": "CPU %",
            "refId": "A"
          },
          {
            "expr": "container_memory_usage_bytes{name=\"gbgreg\"} / container_spec_memory_limit_bytes{name=\"gbgreg\"} * 100",
            "legendFormat": "Memory %", 
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "PostgreSQL Database Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "value"
        },
        "targets": [
          {
            "expr": "pg_up{instance=\"gbgreg:5432\"}",
            "legendFormat": "PostgreSQL Up",
            "refId": "A"
          },
          {
            "expr": "pg_stat_database_numbackends{datname=\"gbgreg_db\"}",
            "legendFormat": "Active Connections",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "DOWN"}}, "type": "value"},
              {"options": {"1": {"text": "UP"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "AI Model Performance",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 8},
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single",
            "sort": "none"
          }
        },
        "targets": [
          {
            "expr": "ollama_request_duration_seconds",
            "legendFormat": "Request Duration (s)",
            "refId": "A"
          },
          {
            "expr": "ollama_tokens_per_second",
            "legendFormat": "Tokens/Second",
            "refId": "B"
          },
          {
            "expr": "ollama_gpu_utilization_percent",
            "legendFormat": "GPU Utilization %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "C# 4.0 Code Generation Stats",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 17},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "minVizWidth": 0,
          "minVizHeight": 75,
          "showUnfilled": true
        },
        "targets": [
          {
            "expr": "gbgreg_code_generation_success_rate",
            "legendFormat": "Success Rate %",
            "refId": "A"
          },
          {
            "expr": "gbgreg_c40_validation_pass_rate", 
            "legendFormat": "C# 4.0 Compliance %",
            "refId": "B"
          },
          {
            "expr": "gbgreg_avg_response_time_seconds",
            "legendFormat": "Avg Response Time (s)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 5,
        "title": "Development Activity Log",
        "type": "logs",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 17},
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": false,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "targets": [
          {
            "expr": "{container=\"gbgreg\"}",
            "refId": "A"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s"
  },
  "overwrite": false
}